{
    "name": "development",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "laravel",
    "workspaceFolder": "/app",
    "forwardPorts": [8000],
    "postCreateCommand": "php artisan serve --host=0.0.0.0",
    "customizations": {
      "vscode": {
        "extensions": [
          "bmewburn.vscode-intelephense-client",
          "xdebug.php-debug",
          "esbenp.prettier-vscode"
        ]
      }
    }
  }